#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin >> n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin >> arr[i];
    }
    for(int i=0;i<n;i++)
    {
        if(arr[i]<38)
            cout << arr[i] << "\n";
        else if((5*((arr[i]/5)+1)-arr[i])<3)
        {
            cout << 5*((arr[i]/5)+1) << "\n";
        }
        else
            cout << arr[i] << "\n";
    }
    return 0;
}

#include<stdio.h>

int main()
{
    int i,a1=0,b1=0;
    int a[3],b[3];
    for(i=0;i<3;i++)
    {
        scanf("%d ", &a[i]);
    }
    for(i=0;i<3;i++)
    {
        scanf("%d ", &b[i]);
    }
    for(i=0;i<3;i++){
    if(a[i]>b[i])
        a1++;
    if(a[i]<b[i])
        b1++;
    if(a[i]==b[i])
        a1==0,b1==0;}
    printf("%d %d",a1, b1); 
    
    return 0; 
}

#include<stdio.h>

int main()
{
    int n,t,i;
    scanf("%d",&n);
    if(n>=1&&n<=1000){
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
     for(i=0;i<n;i++){
         if(a[i]==1)
         {
             printf("2");
         }
         else{
             printf("%d",((a[i])+1));
         }
         printf("\n");
     }   
    }
    
    return 0;
}

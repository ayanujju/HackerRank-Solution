#include <stdio.h>

int main() {
    int n,k,a,b;
    scanf("%d %d", &n, &k);
    int c=0,d=0,e=0;
    
  for (a=1;a<n;a++)
  {
      for (b=n;b>a;b--)
      {
          if ((a&b)<k && (a&b)>c) {c = a&b;}
          if ((a|b)<k && (a|b)>d) {d = a|b;}
          if ((a^b)<k && (a^b)>e) {e = a^b;}
      }
  }
 printf ("%d\n%d\n%d", c,d,e);
 
}

#include<stdio.h>

int factorial(int n)
{
    if(n<=1)
    {
        return 1;
    }
    else
    {
        return (n*factorial(n-1));
    }
    
    return 0;
}

int main()
{
    int n,m;
    scanf("%d",&n);
    if(n>=2&&n<=12)
    {
        m = factorial(n);
        printf("%d",m);
    }
    
    return 0;
}
